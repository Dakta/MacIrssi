		&& exit 0; \
	      exit 1;; \
	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --gnu  src/perl/Makefile'; \
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --gnu  src/perl/Makefile
